  object dsGetEmailAttachments: TADODataSet
    Connection = dtmMain.dbData
    CommandText = 
      'SELECT '#13#10'  EF.FileID,'#13#10'  DF.Description,'#13#10'  DF.Path'#13#10'FROM'#13#10'  DOC' +
      '_File DF'#13#10'  LEFT JOIN DOC_EmailFile EF ON DF.FileID = EF.FileID'#13 +
      #10'  LEFT JOIN DOC_Email DE ON EF.EmailID = DE.EmailID'#13#10'WHERE'#13#10'  D' +
      'E.EmailID = :EmailID'
    Parameters = <
      item
        Name = 'EmailID'
        DataType = ftInteger
        Value = Null
      end>
    Left = 302
    Top = 72
  end
  object dsGetDocumentDetails: TADODataSet
    Connection = dtmMain.dbData
    CommandText = 
      'SELECT  '#13#10'  DD.DocumentID,'#13#10'  DD.Description,'#13#10'  DD.Message,'#13#10'  ' +
      'AL.AccountID '#13#10'FROM DOC_Email DE'#13#10'LEFT JOIN DOC_AccessLogEmail A' +
      'E ON DE.EmailID = AE.EmailID'#13#10'LEFT JOIN DOC_AccessLog AL ON AE.A' +
      'ccessLogID = AL.AccessLogID'#13#10'LEFT JOIN DOC_DocumentVersion DV ON' +
      ' AL.DocumentVersionID = DV.DocumentVersionID'#13#10'LEFT JOIN DOC_Docu' +
      'ment DD ON DV.DocumentID = DD.DocumentID'#13#10'WHERE'#13#10'  DE.EmailID = ' +
      ':EmailID'
    Parameters = <
      item
        Name = 'EmailID'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    Left = 334
    Top = 72
  end
  object dsEmail: TADODataSet
    Connection = dtmMain.dbData
    CursorType = ctStatic
    CommandText = 
      'SELECT '#13#10#9'AL.AccessLogID, AL.DocumentVersionID, AL.AccessTypeID,' +
      ' AL.EntityID, '#13#10#9'AL.AccountID, AL.AccessValue AS EmailAddress, E' +
      'M.Message, EM.Subject, '#13#10#9'AL.Parameter1, AL.Parameter2, AL.Param' +
      'eter3, AL.Parameter4, AL.Parameter5'#13#10'FROM DOC_Email EM'#13#10'LEFT JOI' +
      'N DOC_AccessLogEmail AE ON EM.EmailID = AE.EmailID'#13#10'LEFT JOIN DO' +
      'C_AccessLog AL ON AE.AccessLogID = AL.AccessLogID'#13#10'WHERE EM.Emai' +
      'lID = :EmailID'
    Parameters = <
      item
        Name = 'EmailID'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    Left = 256
    Top = 72
  end
